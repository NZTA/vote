{
  "name": "nzta/vote",
  "description": "A module for voting on a Page OR a Comment",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/NZTA/vote",
  "keywords": [
    "silverstripe",
    "vote",
    "like",
    "cms"
  ],
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/NZTA/vote/issues"
  },
  "require": {
    "silverstripe/cms": "^4.0",
    "silverstripe/framework": "^4.0",
    "colymba/gridfield-bulk-editing-tools": "^3.0",
    "silverstripe/comments": "3.1.0@stable"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7"
  },
  "extra": {
    "installer-name": "vote",
    "expose": [
      "js"
    ]
  },
  "autoload": {
    "psr-4": {
      "NZTA\\Vote\\": "src/",
      "NZTA\\Vote\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
